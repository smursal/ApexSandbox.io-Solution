public String registerStudent(String name, String phone, String email) {
    apxio__Course__c Student= new apxio__Course__c();
    Student.Name = name;
    Student.apxio__Phone__c = phone;
    Student.apxio__Email__c = email;

    insert Student;
}
  String Student.apxio__Registration_Number__c = SELECT apxio__Registration_Number__c from apxio__Student__c
                  WHERE id =:studentList ;
}

