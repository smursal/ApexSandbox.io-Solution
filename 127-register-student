public String registerStudent(String name, String phone, String email) {
    List<apxio__Student__c> studentList = new List<apxio__Student__c>();
    studentList.name = name;
    studentList.apxio__Phone__c = phone;
    studentList.apxio__Email__c = email;

    insert studentList;
}
 studentList.apxio__Registration_Number__c = [SELECT apxio__Registration_Number__c from apxio__Student__c
                  WHERE id =:studentList ];


