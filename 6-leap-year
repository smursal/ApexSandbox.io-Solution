public Boolean isLeapYear(Integer year) {
    Integer results1 = Math.mod(year, 100);
    Integer results2 = Math.mod(year, 400);
    Integer results3 = Math.mod(year, 4);

    if ( results3 != 0 )){
        return false;
    }
    else if (results1 != 0){
        return true;
    }
      else if (results2 != 0){
        return false;

    }
    else return true;
}
